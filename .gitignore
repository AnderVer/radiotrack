/vendor/*
!/vendor/assets/

# Ignore log files
/log/*
!/log/.keep

# Ignore tmp files
/tmp/*
!/tmp/.keep

# Ignore public/uploads
/public/uploads/*
!/public/uploads/.keep

# Ignore dotenv
/.env
/.env.*
!.env.example

# Ignore IDE files
/.idea/
/.vscode/
*.swp
*.swo
*~

# Ignore OS files
.DS_Store
Thumbs.db

# Ignore Docker override
/docker-compose.override.yml

# Ignore test coverage
/coverage

# Ignore Rails credentials and keys
/config/credentials.yml.enc
/config/master.key
/config/credentials/

# Ignore node_modules (if using Webpacker/ESBuild)
/node_modules

# Ignore bootsnap cache
/tmp/cache/bootsnap*

# Ignore storage (except .keep files)
/storage/*
!/storage/.keep

